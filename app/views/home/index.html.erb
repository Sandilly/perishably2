<h1>User's List</h1>
<% if flash[:notice] %>
  <div class="notice"><%= flash[:notice] %></div>
<% end %>
<p> List of Items </p>
<ul>
  <% @products.each do |product|%>
    <li> <a href="/products/<%= product.id %>">
      <%= product.name %> </a> </li>
    <!--<li> <%= product.time %> </li> -->
  <% end %> 
</br>
<%= form_tag(products_path, :method => "get", class: "navbar-form", id: "search-form") do %>
  <div class="input-append">
    <%= text_field_tag :search, params[:search], class: "span2", placeholder: "Search Products" %>
    <button class="btn" type="submit"><i class="btn"></i>Submit</button>
  </div></br>
<% end %>
<%= link_to "Add Item", new_product_path %>
</ul>
  
