<h1>Log in</h1>

<%= form_tag sessions_path do %>

<div class = "field">
  <%= f.label :email %><br>
  <%= f.text_field :email %>
</div>

<div class = "field">
  <%= f.label :password %><br>
  <%= f.password_field :password %>
</div>
<div class = "actions"><%= submit_tag "Log in"%> </div>
<% end %>
