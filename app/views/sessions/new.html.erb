<h1> <!-- <i class="fa fa-arrow-circle-right"></i> -->Log in</h1>
<div class="light well">
  
 
  <%= render "layouts/flash_notices" %>

  <%= form_tag sessions_path do %>
    <div class = "field">
      <%= label_tag :email %>
      <%= text_field_tag :email %>
    </div>

    <div class = "field">
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>

    <div class = "actions">
      <%= submit_tag nil, class: "btn btn-info round2"  do %> 
      <% end %>
    </div>

    
    <%= link_to "Sign Up", signup_path %>
  <% end %>
 

</div>
