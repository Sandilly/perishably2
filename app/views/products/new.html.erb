<h1>Add new items to my list</h1>
 
<%= form_for @product,url: {action: "create"}, html: {class: "new_form"} do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %>

  <%= f.label :expiration_date %>
  <%= f.text_field :expiration_date %>

  <%= link_to "Cancel", root_path %>
  <%= f.submit "Create" %>
<% end %>




